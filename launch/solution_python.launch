<launch>
  <arg name="map_file"       default="/home/sakthi/catkin_ws/src/cse190_comp_robotics/PA2/map/tutorial.yaml"/>
  <arg name="world_file"     default="/home/sakthi/catkin_ws/src/cse190_comp_robotics/PA2/map/tutorial.world"/>
  <arg name="initial_pose_x" default="100.0"/>
  <arg name="initial_pose_y" default="100.0"/>
  <arg name="initial_pose_a" default="0.0"/>

  <node pkg="cse_190_assi_2" type="robot.py" name="particle_filter_localization" output="screen" required="true"/> 
  <node pkg="stage_ros" type="stageros" name="stageros" args="$(arg world_file)">
    <param name="base_watchdog_timeout" value="1000"/>
  </node>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)">
    <param name="frame_id" value="/map"/>
  </node>

</launch>
